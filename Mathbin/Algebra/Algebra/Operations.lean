theorem pow_subset_pow {n : ℕ} : (↑M : Set A) ^ n ⊆ ↑(M ^ n : Submodule R A) := by
  induction' n with n ih
  ·
    erw [pow_zeroₓ, pow_zeroₓ, Set.singleton_subset_iff]
    rw [SetLike.mem_coe, ← one_le]
    exact le_reflₓ _
