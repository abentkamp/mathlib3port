theorem AddMonoidHom.functions_ext [Fintype I] (G : Type _) [AddCommMonoidₓ G] (g h : (∀ i, Z i) →+ G)
    (w : ∀ i : I x : Z i, g (Pi.single i x) = h (Pi.single i x)) : g = h := by
