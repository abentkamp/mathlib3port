theorem geom_sum_def ( x : α ) ( n : ℕ ) : geomSum x n = ∑ i in range n , x ^ i := rfl

theorem geom_sum_succ {x : α} {n : ℕ} : geomSum x (n+1) = (x*geomSum x n)+1 := by
  simp only [geom_sum_def, mul_sum, ← pow_succₓ, sum_range_succ', pow_zeroₓ]

theorem geom_sum_succ' {x : α} {n : ℕ} : geomSum x (n+1) = (x ^ n)+geomSum x n :=
  (sum_range_succ _ _).trans (add_commₓ _ _)

@[simp]
theorem geom_sum_zero (x : α) : geomSum x 0 = 0 :=
  rfl

@[simp]
theorem geom_sum_one (x : α) : geomSum x 1 = 1 := by
  simp [geom_sum_succ']

@[simp]
theorem geom_sum_two {x : α} : geomSum x 2 = x+1 := by
  simp [geom_sum_succ']

@[simp]
theorem zero_geom_sum : ∀ {n}, geomSum (0 : α) n = if n = 0 then 0 else 1
  | 0 => by
    simp
  | 1 => by
    simp
  | n+2 => by
    rw [geom_sum_succ']
    simp [zero_geom_sum]

@[simp]
theorem one_geom_sum (n : ℕ) : geomSum (1 : α) n = n := by
  simp [geom_sum_def]

@[simp]
theorem op_geom_sum (x : α) (n : ℕ) : op (geomSum x n) = geomSum (op x) n := by
  simp [geom_sum_def]

