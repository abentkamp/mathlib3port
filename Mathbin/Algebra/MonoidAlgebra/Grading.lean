  def of_grades : ⨁ i : ι , grade R i →ₐ[ R ] AddMonoidAlgebra R ι := of_grades_by AddMonoidHom.id ι

@[simp]
theorem of_grades_by_of (i : ι) (x : grade_by R f i) :
    of_grades_by f (DirectSum.of (fun i => grade_by R f i) i x) = x :=
  DirectSum.submodule_coe_alg_hom_of (grade_by R f) i x

@[simp]
theorem of_grades_of (i : ι) (x : grade R i) : of_grades (DirectSum.of (fun i => grade R i) i x) = x := by
  apply of_grades_by_of

