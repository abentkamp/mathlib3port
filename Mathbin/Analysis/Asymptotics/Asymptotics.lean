  ⟨fun h => ⟨h.prod_left_fst, h.prod_left_snd⟩, fun h => h.1.prod_left_same h.2⟩

theorem is_O.prod_left (hf : is_O f' k' l) (hg : is_O g' k' l) : is_O (fun x => (f' x, g' x)) k' l :=
  let ⟨c, hf⟩ := hf.is_O_with
  let ⟨c', hg⟩ := hg.is_O_with
  (hf.prod_left hg).IsO

