  ⟨fun π₁ π₂ => π₁.bUnion fun J => π₂.restrict J⟩

theorem inf_def (π₁ π₂ : prepartition I) : π₁⊓π₂ = π₁.bUnion fun J => π₂.restrict J :=
  rfl

