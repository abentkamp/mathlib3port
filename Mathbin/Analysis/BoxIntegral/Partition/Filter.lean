    simpa [π₂, hπ₁U] using (disjoint_diff.mono_left sdiff_le).symm
  refine' ⟨π₁.disj_union π₂.to_prepartition this, _, _⟩
  ·
    suffices ↑I \ π.Union ∪ π.Union \ (π.filter p).Union = ↑I \ (π.filter p).Union by
      simpa [*]
