    simp only [comp_apply, LinearEquiv.coe_coe, LinearEquiv.of_top_symm_apply]
  ·
    exact fun x => (hfg (Submodule.coe_mk _ _).symm).symm
  ·
    exact fun x hx => hgs ⟨x, _⟩ hx

