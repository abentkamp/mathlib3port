  convert hc
  have : ∥x₀∥ ≠ 0 := by
    simp [hx₀]
  field_simp
  simpa [inner_smul_left, real_inner_self_eq_norm_mul_norm, sq] using congr_argₓ (fun x => ⟪x, x₀⟫_ℝ) hc

