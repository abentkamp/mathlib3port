  ( x y : Σ j , F.obj j ) : filtered_colimit.rel F x y → EqvGen quot.rel F x y
  :=
    fun
      ⟨ k , f , g , h ⟩
        =>
        EqvGen.trans _ ⟨ k , F.map f x . 2 ⟩ _ EqvGen.rel _ _ ⟨ f , rfl ⟩ EqvGen.symm _ _ EqvGen.rel _ _ ⟨ g , h ⟩

