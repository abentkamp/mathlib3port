  fold_add
  ( b₁ b₂ : α ) ( s₁ s₂ : Multiset α ) : s₁ + s₂ . fold op b₁ * b₂ = s₁.fold op b₁ * s₂.fold op b₂
  :=
    Multiset.induction_on
      s₂
        by rw [ add_zeroₓ , fold_zero , ← fold_cons'_right , ← fold_cons_right op ]
        by simp ( config := { contextual := Bool.true._@._internal._hyg.0 } ) <;> cc

theorem fold_singleton (b a : α) : ({a} : Multiset α).fold op b = a*b :=
  foldr_singleton _ _ _ _

