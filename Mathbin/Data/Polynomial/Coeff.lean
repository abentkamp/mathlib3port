@[ simp ]
  theorem
    finset_sum_coeff
    { ι : Type _ } ( s : Finset ι ) ( f : ι → Polynomial R ) ( n : ℕ ) : coeff ∑ b in s , f b n = ∑ b in s , coeff f b n
    := lcoeff R n . map_sum

theorem coeff_sum [Semiringₓ S] (n : ℕ) (f : ℕ → R → Polynomial S) :
    coeff (p.sum f) n = p.sum fun a b => coeff (f a b) n := by
  rcases p with ⟨⟩
  simp [Polynomial.sum, support, coeff]

