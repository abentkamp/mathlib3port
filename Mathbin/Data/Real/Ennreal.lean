theorem lt_add_right (ha : a ≠ ∞) (hb : b ≠ 0) : a < a+b := by
  rwa [← pos_iff_ne_zero, ← add_lt_add_iff_left ha, add_zeroₓ] at hb

theorem le_of_forall_pos_le_add : ∀ {a b : ℝ≥0∞}, (∀ ε : ℝ≥0 , 0 < ε → b < ∞ → a ≤ b+ε) → a ≤ b
  | a, none, h => le_top
  | none, some a, h =>
