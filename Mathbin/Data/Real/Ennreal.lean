  ·
    simp
  rw [pow_succₓ, mul_eq_top, ihn]
  fconstructor
  ·
    rintro (⟨-, rfl, h0⟩ | ⟨rfl, h0⟩) <;> exact ⟨rfl, n.succ_ne_zero⟩
  ·
    rintro ⟨rfl, -⟩
    exact Or.inr ⟨rfl, pow_ne_zero n top_ne_zero⟩

