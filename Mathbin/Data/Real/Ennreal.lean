  simpa only [inv_invₓ] using @inv_lt_inv a (b⁻¹)

theorem lt_inv_iff_lt_inv : a < b⁻¹ ↔ b < a⁻¹ := by
  simpa only [inv_invₓ] using @inv_lt_inv (a⁻¹) b

@[simp]
theorem inv_le_inv : a⁻¹ ≤ b⁻¹ ↔ b ≤ a := by
