theorem pairwise_disjoint_fiber (f : ι → α) (s : Set α) : s.pairwise_disjoint fun a => f ⁻¹' {a} :=
  fun a _ b _ h i ⟨hia, hib⟩ => h $ (Eq.symm hia).trans hib

