theorem expand_contract [NoZeroDivisors R] {f : Polynomial R} (hf : f.derivative = 0) (hp : p ≠ 0) :
    expand R p (contract p f) = f := by
  ext n
  rw [coeff_expand hp.bot_lt, coeff_contract hp]
