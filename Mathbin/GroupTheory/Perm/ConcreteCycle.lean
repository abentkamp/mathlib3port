instance repr_perm [HasRepr α] : HasRepr (perm α) :=
  ⟨fun f =>
    reprₓ
      (Multiset.pmap (fun g : perm α hg : g.is_cycle => iso_cycle ⟨g, hg⟩) (perm.cycle_factors_finset f).val fun g hg =>
        (mem_cycle_factors_finset_iff.mp (Finset.mem_def.mpr hg)).left)⟩

end Equivₓ.Perm

