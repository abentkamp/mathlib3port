theorem sign_aux_swap : ∀ {n : ℕ} {x y : Finₓ n} hxy : x ≠ y, sign_aux (swap x y) = -1
  | 0 => by
    decide
  | 1 => by
    decide
  | n+2 => fun x y hxy =>
    have h2n : 2 ≤ n+2 := by
      decide
    by
    rw [← is_conj_iff_eq, ← sign_aux_swap_zero_one h2n]
    exact
      (MonoidHom.mk' sign_aux sign_aux_mul).map_is_conj
