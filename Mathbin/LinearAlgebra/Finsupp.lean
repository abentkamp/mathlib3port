  ·
    simp
  ·
    intro f g hf hg
    simp [map_add, hf, hg]
  ·
    intro k m
    simp only [Finsupp.lcongr_symm_single]
    simp only [Finsupp.single, Equivₓ.symm_apply_eq, Finsupp.coe_mk]
    split_ifs <;> simp

