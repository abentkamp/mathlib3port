theorem lcongr_symm {ι κ : Sort _} (e₁ : ι ≃ κ) (e₂ : M ≃ₗ[R] N) : (lcongr e₁ e₂).symm = lcongr e₁.symm e₂.symm := by
  ext f i
  simp only [Equivₓ.symm_symm, Finsupp.lcongr_apply_apply]
