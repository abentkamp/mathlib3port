theorem
  charpoly_monic
  ( M : Matrix n n R ) : M.charpoly.monic
  :=
    by
      nontriviality
        by_cases' Fintype.card n = 0
        · rw [ charpoly , det_of_card_zero h ] apply monic_one
        have
          mon
            : ∏ i : n , X - C M i i . Monic
            :=
            by apply monic_prod_of_monic univ fun i : n => X - C M i i simp [ monic_X_sub_C ]
        rw [ ← sub_add_cancel ∏ i : n , X - C M i i M.charpoly ] at mon
        rw [ monic ] at *
        rw [ leading_coeff_add_of_degree_lt ] at mon
        rw [ ← mon ]
        rw [ charpoly_degree_eq_dim ]
        rw [ ← neg_sub ]
        rw [ degree_neg ]
        apply lt_transₓ charpoly_sub_diagonal_degree_lt M
        rw [ WithBot.coe_lt_coe ]
