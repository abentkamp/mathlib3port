          ⟨fun m => n • f m, fun m i x y => by
            simp [smul_add], fun l i x d => by
            simp [← smul_comm x n]⟩,
        zsmul_zero' := _, zsmul_succ' := _, zsmul_neg' := _, .. } <;>
