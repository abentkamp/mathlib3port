theorem ae_cover.restrict {φ : ι → Set α} (hφ : ae_cover μ l φ) {s : Set α} : ae_cover (μ.restrict s) l φ :=
  { ae_eventually_mem := ae_restrict_of_ae hφ.ae_eventually_mem, Measurable := hφ.measurable }

