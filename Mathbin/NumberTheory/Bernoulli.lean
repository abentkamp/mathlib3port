                ∑ i in range p , bernoulli i + 2 * p + 2 . choose i + 2 * n ^ p - i / ↑ p + 2
                  =
                  ∑ i in range p , bernoulli' i + 2 * p + 2 . choose i + 2 * n ^ p - i / ↑ p + 2
              :=
              sum_congr rfl fun i h => by rw [ bernoulli_eq_bernoulli'_of_ne_one succ_succ_ne_one i ]
          calc
            ∑ k in Ico 1 n.succ , ↑ k ^ p.succ = ∑ k in range n.succ , ↑ k ^ p.succ
                :=
                by simp [ sum_Ico_eq_sub _ hle , succ_ne_zero ]
              _ = ∑ k in range n , ( k : ℚ ) ^ p.succ + n ^ p.succ := by rw [ sum_range_succ ]
              _ = ∑ i in range p.succ.succ , f i + n ^ p.succ := by simp [ f , sum_range_pow ]
              _ = ∑ i in range p , f i.succ.succ + f 1 + f 0 + n ^ p.succ := by simp_rw [ sum_range_succ' ]
              _ = ∑ i in range p , f i.succ.succ + f 1 + n ^ p.succ + f 0 := by ring
              _ = ∑ i in range p , f i.succ.succ + 1 / 2 * n ^ p.succ + f 0 := by rw [ h2 ]
              _ = ∑ i in range p , f' i.succ.succ + f' 1 + f' 0 := by simp only [ f , f' ] simpa [ h1 ]
              _ = ∑ i in range p.succ.succ , f' i := by simp_rw [ sum_range_succ' ]

end Faulhaber

