theorem divisors_prime_pow {p : ℕ} (pp : p.prime) (k : ℕ) :
    divisors (p ^ k) = (Finset.range (k+1)).map ⟨pow p, pow_right_injective pp.two_le⟩ := by
  ext
  simp [mem_divisors_prime_pow, pp, Nat.lt_succ_iffₓ, @eq_comm _ a]

