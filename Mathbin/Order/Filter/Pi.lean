  exact ⟨{ x : α i | m i x ∈ t }, H, fun x hx => hH hx⟩

theorem tendsto.pi_map_Coprod {g : ∀ i, Filter (β i)} (h : ∀ i, tendsto (m i) (f i) (g i)) :
    tendsto (fun k : ∀ i, α i => fun i => m i (k i)) (Filter.coprodₓ f) (Filter.coprodₓ g) :=
