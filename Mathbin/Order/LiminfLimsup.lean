theorem
  frequently_lt_of_liminf_lt
  { α β }
      [ ConditionallyCompleteLinearOrder β ]
      { f : Filter α }
      { u : α → β }
      { b : β }
      ( hu : f.is_cobounded_under · ≥ · u := by run_tac is_bounded_default )
      ( h : f.liminf u < b )
    : ∃ᶠ x in f , u x < b
  := @ frequently_lt_of_lt_limsup _ OrderDual β _ f u b hu h

end ConditionallyCompleteLinearOrder

