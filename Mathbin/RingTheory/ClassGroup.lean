theorem ClassGroup.mk0_eq_one_iff [IsDedekindDomain R] {I : Ideal R} (hI : I ∈ (Ideal R)⁰) :
    ClassGroup.mk0 K ⟨I, hI⟩ = 1 ↔ I.is_principal :=
  ClassGroup.mk_eq_one_iff.trans (coe_submodule_is_principal R K)

