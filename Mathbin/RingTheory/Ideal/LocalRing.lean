theorem is_unit_map_iff [Semiringₓ R] [Semiringₓ S] (f : R →+* S) [IsLocalRingHom f] a : IsUnit (f a) ↔ IsUnit a :=
  ⟨IsLocalRingHom.map_nonunit a, f.is_unit_map⟩

