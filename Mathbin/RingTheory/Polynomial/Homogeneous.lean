theorem
  is_homogeneous_monomial
  ( d : σ →₀ ℕ ) ( r : R ) ( n : ℕ ) ( hn : ∑ i in d.support , d i = n ) : is_homogeneous monomial d r n
  := by intro c hc classical rw [ coeff_monomial ] at hc split_ifs at hc with h · subst c exact hn · contradiction

variable (σ) {R}

theorem is_homogeneous_C (r : R) : is_homogeneous (C r : MvPolynomial σ R) 0 := by
  apply is_homogeneous_monomial
  simp only [Finsupp.zero_apply, Finset.sum_const_zero]

variable (σ R)

theorem is_homogeneous_zero (n : ℕ) : is_homogeneous (0 : MvPolynomial σ R) n :=
