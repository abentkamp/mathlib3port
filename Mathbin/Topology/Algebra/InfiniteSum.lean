protected
  theorem
    HasSum.map
    [ AddCommMonoidₓ γ ] [ TopologicalSpace γ ] ( hf : HasSum f a ) ( g : α →+ γ ) ( hg : Continuous g )
      : HasSum g ∘ f g a
    :=
      have
