  has_sum_single
  { f : β → α } ( b : β ) ( hf : ∀ b' _ : b' ≠ b , f b' = 0 ) : HasSum f f b
  := suffices HasSum f ∑ b' in { b } , f b' by simpa using this has_sum_sum_of_ne_finset_zero $ by simpa [ hf ]

theorem has_sum_ite_eq (b : β) [DecidablePred (· = b)] (a : α) : HasSum (fun b' => if b' = b then a else 0) a := by
