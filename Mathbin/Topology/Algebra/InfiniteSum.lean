theorem
  HasSum.sigma_of_has_sum
  [ RegularSpace α ]
      { γ : β → Type _ }
      { f : Σ b : β , γ b → α }
      { g : β → α }
      { a : α }
      ( ha : HasSum g a )
      ( hf : ∀ b , HasSum fun c => f ⟨ b , c ⟩ g b )
      ( hf' : Summable f )
    : HasSum f a
  := by simpa [ hf'.has_sum.sigma hf . unique ha ] using hf'.has_sum

end HasSum

