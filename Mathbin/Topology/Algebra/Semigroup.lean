    continuous_subtype_mk _ ((continuous_mul_left p.1).comp continuous_subtype_val)
  obtain ⟨⟨m, hm⟩, idem⟩ := exists_idempotent_of_compact_t2_of_continuous_mul_left this
  exact ⟨m, hm, subtype.ext_iff.mp idem⟩

