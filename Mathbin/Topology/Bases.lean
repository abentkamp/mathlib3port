protected theorem is_topological_basis.is_open {s : Set Œ±} {b : Set (Set Œ±)} (hb : is_topological_basis b)
    (hs : s ‚àà b) : IsOpen s := by
  rw [hb.eq_generate_from]
  exact generate_open.basic s hs

protected theorem is_topological_basis.mem_nhds {a : Œ±} {s : Set Œ±} {b : Set (Set Œ±)} (hb : is_topological_basis b)
    (hs : s ‚àà b) (ha : a ‚àà s) : s ‚àà ùìù a :=
  (hb.is_open hs).mem_nhds ha

theorem is_topological_basis.exists_subset_of_mem_open {b : Set (Set Œ±)} (hb : is_topological_basis b) {a : Œ±}
    {u : Set Œ±} (au : a ‚àà u) (ou : IsOpen u) : ‚àÉ v ‚àà b, a ‚àà v ‚àß v ‚äÜ u :=
  hb.mem_nhds_iff.1 $ IsOpen.mem_nhds ou au

