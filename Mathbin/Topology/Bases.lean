theorem is_topological_basis.nhds_has_basis {b : Set (Set Œ±)} (hb : is_topological_basis b) {a : Œ±} :
    (ùìù a).HasBasis (fun t : Set Œ± => t ‚àà b ‚àß a ‚àà t) fun t => t :=
  ‚ü®fun s =>
