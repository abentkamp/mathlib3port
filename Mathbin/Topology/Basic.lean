  calc IsClosed s ↔ Closure s ⊆ s := closure_subset_iff_is_closed.symm
    _ ↔ ∀ a, ClusterPt a (𝓟 s) → a ∈ s := by
    simp only [subset_def, mem_closure_iff_cluster_pt]
    

