/--  A topology on `α`. -/
@[protect_proj]
structure TopologicalSpace (α : Type u) where
  IsOpen : Set α → Prop
  is_open_univ : IsOpen univ
  is_open_inter : ∀ s t, IsOpen s → IsOpen t → IsOpen (s ∩ t)
  is_open_sUnion : ∀ s, (∀, ∀ t ∈ s, ∀, IsOpen t) → IsOpen (⋃₀s)

attribute [class] TopologicalSpace

